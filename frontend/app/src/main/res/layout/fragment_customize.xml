<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EEEEEE">

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:gravity="center_horizontal"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/rounded_corner_view_lightpurple"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="20dp"
                app:srcCompat="@drawable/penguin" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="503dp"
            android:layout_margin="10dp"
            android:background="@drawable/rounded_corner"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="20dp"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:layout_marginLeft="10dp"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/mtextView7"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="color" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/mbutton8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:backgroundTint="#E9BCF0" />

                    <Button
                        android:id="@+id/nbutton8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:backgroundTint="#B0EEEE" />

                    <Button
                        android:id="@+id/obutton8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:backgroundTint="#EEB0B0" />

                    <Button
                        android:id="@+id/pbutton8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:backgroundTint="#E4EB91" />

                    <Button
                        android:id="@+id/qbutton8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:backgroundTint="#D4EEB0" />

                    <ImageView
                        android:id="@+id/mtest"
                        android:layout_width="30dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="2dp"
                        android:alpha="0.53"
                        android:padding="3dp"
                        app:srcCompat="@drawable/ic_baseline_arrow_forward_ios_24" />

                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="20dp"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:layout_marginLeft="10dp"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/atextView7"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="animal" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/bbutton8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:src="@drawable/penguin" />

                    <ImageView
                        android:id="@+id/cbutton8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:src="@drawable/pig" />

                    <ImageView
                        android:id="@+id/dbutton8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:src="@drawable/hamster" />

                    <ImageView
                        android:id="@+id/ebutton8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:src="@drawable/rabbit" />

                    <ImageView
                        android:id="@+id/fbutton8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:src="@drawable/lion" />

                    <ImageView
                        android:id="@+id/gtest"
                        android:layout_width="30dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="2dp"
                        android:alpha="0.53"
                        android:padding="3dp"
                        app:srcCompat="@drawable/ic_baseline_arrow_forward_ios_24" />

                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="20dp"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:layout_marginLeft="10dp"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/qtextView7"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="hats" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/rbutton8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:src="@drawable/tophat" />

                    <ImageView
                        android:id="@+id/sbutton8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:src="@drawable/cap" />

                    <ImageView
                        android:id="@+id/tbutton8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:src="@drawable/womanhat" />

                    <ImageView
                        android:id="@+id/ubutton8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:src="@drawable/crown" />

                    <ImageView
                        android:id="@+id/vbutton8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:src="@drawable/picnichat" />

                    <ImageView
                        android:id="@+id/wtest"
                        android:layout_width="30dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="2dp"
                        android:alpha="0.53"
                        android:padding="3dp"
                        app:srcCompat="@drawable/ic_baseline_arrow_forward_ios_24" />

                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="20dp"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:layout_marginLeft="10dp"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textView7"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="clothing" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/z1button8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:src="@drawable/tshirt" />

                    <ImageView
                        android:id="@+id/z2button8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:src="@drawable/tanktop" />

                    <ImageView
                        android:id="@+id/z3button8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:src="@drawable/buttonup" />

                    <ImageView
                        android:id="@+id/z4button8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:src="@drawable/blouse" />

                    <ImageView
                        android:id="@+id/z5button8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:src="@drawable/dress" />

                    <ImageView
                        android:id="@+id/ztest"
                        android:layout_width="30dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="2dp"
                        android:alpha="0.53"
                        android:padding="3dp"
                        app:srcCompat="@drawable/ic_baseline_arrow_forward_ios_24" />

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>